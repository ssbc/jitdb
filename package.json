{
  "name": "jitdb",
  "description": "A database on top of a flumelog with automatic index generation and maintenance",
  "version": "0.27.0",
  "homepage": "https://github.com/arj03/jitdb",
  "repository": {
    "type": "git",
    "url": "git://github.com/arj03/jitdb.git"
  },
  "dependencies": {
    "atomically-universal": "^0.1.0",
    "binary-search-bounds": "^2.0.4",
    "bipf": "^1.4.0",
    "debug": "^4.2.0",
    "idb-kv-store": "^4.5.0",
    "jsesc": "^3.0.2",
    "mkdirp": "^1.0.4",
    "multicb": "1.2.2",
    "promisify-4loc": "1.0.0",
    "pull-async": "~1.0.0",
    "pull-awaitable": "^1.0.0",
    "pull-cat": "~1.1.11",
    "pull-stream": "^3.6.14",
    "push-stream-to-pull-stream": "^1.0.3",
    "push-stream": "^11.0.0",
    "sanitize-filename": "^1.6.3",
    "traverse": "^0.6.6",
    "typedarray-to-buffer": "^3.1.5",
    "typedfastbitset": "^0.1.5"
  },
  "devDependencies": {
    "async-flumelog": "^1.0.11",
    "flumecodec": "0.0.1",
    "flumelog-offset": "3.4.4",
    "husky": "^4.3.0",
    "obv": "0.0.1",
    "prettier": "^2.1.2",
    "pretty-quick": "^3.1.0",
    "pull-pushable": "^2.2.0",
    "rimraf": "^3.0.2",
    "ssb-fixtures": "2.2.0",
    "ssb-keys": "^7.2.2",
    "ssb-ref": "^2.14.0",
    "ssb-validate": "^4.1.1",
    "tap-spec": "^5.0.0",
    "tape": "^5.0.1"
  },
  "scripts": {
    "test": "tape test/*.js | tap-spec",
    "format-code": "prettier --write \"*.js\" \"test/*.js\"",
    "format-code-staged": "pretty-quick --staged --pattern \"*.js\" --pattern \"test/*.js\"",
    "benchmark": "node benchmark/index.js | tap-spec",
    "benchmark-no-create": "node benchmark/index.js noCreate | tap-spec"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run format-code-staged"
    }
  },
  "author": "Anders Rune Jensen <arj03@protonmail.ch>",
  "contributors": [
    "Andre Staltz <contact@staltz.com>"
  ],
  "license": "LGPL-3.0"
}
